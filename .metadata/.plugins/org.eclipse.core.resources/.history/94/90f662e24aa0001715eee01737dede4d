package com.letscatchup.user.domain.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.IdClass;
import javax.persistence.Table;

@Entity
@IdClass(AttendeeId.class)
@Table(name="LCU_EV_ATTENDEES")
public class AttendeeEntity {

	private AttendeeId attendeeId;
	
	@Id
	@Column(name="EVA_USER_ID")
	private long userId;
	
	@Id
	@Column(name="EVA_EV_ID")
	private long eventId;

	public long getUserId() {
		return userId;
	}

	public void setUserId(long userId) {
		this.userId = userId;
	}

	public long getEventId() {
		return eventId;
	}

	public void setEventId(long eventId) {
		this.eventId = eventId;
	}

	public AttendeeId getAttendeeId() {
		return attendeeId;
	}

	public void setAttendeeId(AttendeeId attendeeId) {
		this.attendeeId = attendeeId;
	}
	
}

class AttendeeId {
	long userId;
	
	long eventId;
	
}
